@prefix : <http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#> .

<http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente> rdf:type owl:Ontology ;
                                                                          owl:imports <http://purl.org/net/cartCoord#> ;
                                                                          <http://creativecommons.org/ns#license> "https://creativecommons.org/licenses/by/4.0/" ;
                                                                          <http://purl.org/dc/elements/1.1/creator> "Jorge, Pablo y Arturo" ;
                                                                          <http://purl.org/dc/elements/1.1/description> "Ontología para describir ciudades, hoteles, transportes y eventos turísticos inteligentes en España" ;
                                                                          <http://purl.org/dc/elements/1.1/modified> "2025-04-27^^xsd:date" ;
                                                                          <http://purl.org/vocab/vann/preferredNamespacePrefix> "http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#" ,
                                                                                                                                "ti" ;
                                                                          rdfs:label "Turismo Inteligente Ontology" .

#################################################################
#    Annotation properties
#################################################################

###  http://creativecommons.org/ns#license
<http://creativecommons.org/ns#license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/modified
<http://purl.org/dc/elements/1.1/modified> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/net/cartCoord#located_in
<http://purl.org/net/cartCoord#located_in> rdf:type owl:ObjectProperty ,
                                                    owl:FunctionalProperty ;
                                           rdfs:domain :City ;
                                           rdfs:range <http://purl.org/net/cartCoord#Point> ;
                                           rdfs:comment "Localización geográfica"@es ;
                                           rdfs:label "located_in_point" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#cooking_style
:cooking_style rdf:type owl:ObjectProperty ;
               rdfs:domain :Restaurant ;
               rdfs:range :Cuisine ;
               rdfs:comment "Estilo de cocina de un restaurante"@es ;
               rdfs:label "cooking_style" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#has_day
:has_day rdf:type owl:ObjectProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :Date ;
         rdfs:range :Day ;
         rdfs:comment "Es el día indicado"@es ;
         rdfs:label "has_day" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#has_month
:has_month rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Date ;
           rdfs:range :Month ;
           rdfs:comment "Es el mes indicado"@es ;
           rdfs:label "has_month" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#has_year
:has_year rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Date ;
          rdfs:range :Year ;
          rdfs:comment "Es el año indicado"@es ;
          rdfs:label "has_year" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#is_categorized
:is_categorized rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Hotel ;
                rdfs:range :HotelCategory ;
                rdfs:comment "El hotel es de la categoría relacionada."@es ;
                rdfs:label "has_category" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#located_in_City
:located_in_City rdf:type owl:ObjectProperty ;
                 rdfs:range :City ;
                 rdfs:comment "La entidad pertenece a la ciudad indicada"@es ;
                 rdfs:label "located_in_city" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#occurs_in_City
:occurs_in_City rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Attraction ;
                rdfs:range :City ;
                rdfs:comment "Indica en qué ciudad tiene lugar un evento"@es ;
                rdfs:label "occurs_in_city" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#occurs_in_date
:occurs_in_date rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Event ;
                rdfs:range :Date ;
                rdfs:comment "Fecha en la que ocurre un evento"@es ;
                rdfs:label "ocurrs_in_date" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#rated
:rated rdf:type owl:ObjectProperty ;
       rdfs:range :Rating ;
       rdfs:comment "Propiedad de ser puntuado por un usuario"@es ;
       rdfs:label "is_rated" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#runned_by
:runned_by rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Transport ;
           rdfs:range :transport_company ;
           rdfs:comment "Relaciona el medio de transporte con la empresa operadora"@es ;
           rdfs:label "runned_by" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#type_of_attraction
:type_of_attraction rdf:type owl:ObjectProperty ;
                    rdfs:domain :Attraction ;
                    rdfs:range :AttractionType ;
                    rdfs:comment "Indica qué tipo de atracción es"@es ;
                    rdfs:label "is_attraction_type" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#type_of_event
:type_of_event rdf:type owl:ObjectProperty ;
               rdfs:domain :Event ;
               rdfs:range :EventType ;
               rdfs:comment "Indica el tipo de evento"@es ;
               rdfs:label "is_event_type" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#type_of_hotel
:type_of_hotel rdf:type owl:ObjectProperty ;
               rdfs:domain :Hotel ;
               rdfs:range :HotelType ;
               rdfs:comment "Indica o vincula el tipo de establecimiento hotelero"@es ;
               rdfs:label "is_hotel_type" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#type_of_transportation
:type_of_transportation rdf:type owl:ObjectProperty ;
                        rdfs:domain :Transport ;
                        rdfs:range :TransportationMean ;
                        rdfs:comment "Vincula un transporte real con el medio de transporte que usa"@es ;
                        rdfs:label "is_transportation_type" .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/net/cartCoord#xcoord
<http://purl.org/net/cartCoord#xcoord> rdfs:comment "Corresponde a la coordenada X o longitud."@es .


###  http://purl.org/net/cartCoord#ycoord
<http://purl.org/net/cartCoord#ycoord> rdfs:comment "Corresponde a la coordenada Y o latitud"@es .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#Price
:Price rdf:type owl:DatatypeProperty ;
       rdfs:range xsd:decimal ;
       rdfs:comment "Indica el valor numérico del precio"@es ;
       rdfs:label "price" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#Score
:Score rdf:type owl:DatatypeProperty ;
       rdfs:range xsd:decimal ;
       rdfs:comment "Indica el valor de la puntuación otorgada"@es ;
       rdfs:label "score" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#day_of_month
:day_of_month rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Day ;
              rdfs:range xsd:positiveInteger ;
              rdfs:comment "Valor numérico del día del mes"@es ;
              rdfs:label "day_month" .


#################################################################
#    Classes
#################################################################

###  http://purl.org/net/cartCoord#Point
<http://purl.org/net/cartCoord#Point> rdfs:comment "Representación de un punto geográfico en la forma latitud, longitud"@es ;
                                      rdfs:label "point" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#Attraction
:Attraction rdf:type owl:Class ;
            rdfs:comment "Representa una atracción como jardín o playa"@es ;
            rdfs:label "attraction" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#AttractionType
:AttractionType rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Class ;
                                      owl:oneOf ( :Beach
                                                  :Cultural
                                                  :Garden
                                                  :Monument
                                                  :Museum
                                                )
                                    ] ;
                rdfs:comment "Tipo de atracción, como playa o museo"@es ;
                rdfs:label "attraction_type" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#City
:City rdf:type owl:Class ;
      rdfs:comment "Representa una ciudad física"@es ;
      rdfs:label "city" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#Cuisine
:Cuisine rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:oneOf ( :Andalusian
                                           :Italian
                                           :Japanese
                                           :Mediterranean
                                           :Mexican
                                         )
                             ] ;
         rdfs:comment "Representa un estilo de comida, como mejicana o japonesa."@es ;
         rdfs:label "cuisine" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#Date
:Date rdf:type owl:Class ;
      rdfs:comment "Representa una fecha."@es ;
      rdfs:label "date" ;
      rdfs:sameAs "https://www.ontotext.com/proton/protonext.html#Date" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#Day
:Day rdf:type owl:Class ;
     rdfs:comment "Representa un día del mes"@es ;
     rdfs:label "day" ;
     rdfs:sameAs "http://www.ontotext.com/proton/protonext#DayOfMonth" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#Event
:Event rdf:type owl:Class ;
       rdfs:comment "Representa un evento o suceso que puede ser de interés turístico"@es ;
       rdfs:label "event" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#EventType
:EventType rdf:type owl:Class ;
           rdfs:comment "Clase que representa un tipo de evento, como musical o tecnológico."@es ;
           rdfs:label "event_type" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#Hotel
:Hotel rdf:type owl:Class ;
       rdfs:comment "Representación de un alojamiento para viajeros."@es ;
       rdfs:label "hotel" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#HotelCategory
:HotelCategory rdf:type owl:Class ;
               rdfs:comment "Categoría corriente en estrellas de los alojamientos"@es ;
               rdfs:label "hotel_category" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#HotelType
:HotelType rdf:type owl:Class ;
           rdfs:comment "Representa un tipo de hotel: playero, lujoso, rural, etc."@es ;
           rdfs:label "hotel_type" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#Month
:Month rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Class ;
                             owl:oneOf ( :April
                                         :August
                                         :December
                                         :February
                                         :January
                                         :July
                                         :June
                                         :March
                                         :May
                                         :November
                                         :October
                                         :September
                                       )
                           ] ;
       rdfs:comment "Representa un mes del año"@es ;
       rdfs:label "month" ;
       rdfs:sameAs "http://www.ontotext.com/proton/protonext#Month" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#Rating
:Rating rdf:type owl:Class ;
        rdfs:comment "Representa una puntuación o valoración por parte de un humano"@es ;
        rdfs:label "rating" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#Restaurant
:Restaurant rdf:type owl:Class ;
            rdfs:comment "Representa un establecimiento de restauración"@es ;
            rdfs:label "restaurant" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#Transport
:Transport rdf:type owl:Class ;
           rdfs:comment "Representación de un sistema de trasporte directamente utilizable."@es ;
           rdfs:label "transport" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#TransportationMean
:TransportationMean rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Class ;
                                          owl:oneOf ( :Bus
                                                      :Metro
                                                      :PublicCycle
                                                      :Taxi
                                                      :Trolley
                                                    )
                                        ] ;
                    rdfs:comment "Medio de transporte genérico, como \"tranvía\" o \"taxi\"."@es ;
                    rdfs:label "transportation_mean" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#Year
:Year rdf:type owl:Class ;
      rdfs:comment "Representa un año del calendario."@es ;
      rdfs:label "year" ;
      rdfs:sameAs "http://www.ontotext.com/proton/protonext#CalendarYear" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#expensive_restaurant
:expensive_restaurant rdf:type owl:Class ;
                      rdfs:subClassOf :Restaurant ;
                      rdfs:comment "Restaurante considerado \"caro\"."@es ;
                      rdfs:label "expensive_restaurant" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#practical_city
:practical_city rdf:type owl:Class ;
                rdfs:subClassOf :City ;
                rdfs:comment "Una especialización de ciudad que se sabe que ofrece al menos un hotel, transporte y restaurante."@es ;
                rdfs:label "practical_city" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#pre_season_event
:pre_season_event rdf:type owl:Class ;
                  rdfs:subClassOf :Event ;
                  rdfs:comment "Una especialización de evento que comienza justo en Junio."@es ;
                  rdfs:label "pre_season_event" .


###  http://www.semanticweb.org/jorge/ontologies/2025/3/turismo-inteligente#transport_company
:transport_company rdf:type owl:Class ;
                   rdfs:comment "Empresa de transportes"@es ;
                   rdfs:label "transport_company" .


#################################################################
#    Individuals
#################################################################

